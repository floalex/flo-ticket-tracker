<%= render 'shared/errors', obj: @project %>

<div class='row'>
  <div class="col-sm-10">
    <%= bootstrap_form_for @project do |f| %>
      <%= f.text_field :name %>
      <%= f.text_area :description %>
      <%= f.form_group do %>
        <%= f.submit 'Submit', class: "btn btn-success" %>
      <% end %>
    <% end %>
  </div>
</div>